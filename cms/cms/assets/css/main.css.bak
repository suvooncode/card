.search-area {
    position: fixed;
    top: 00%;
    left: 0px;
    width: 100%;
    background-color: #fff;
    z-index: 9999999999999;
    opacity: 0;
    height: 100%;
    visibility: hidden;
    transition: all 0.5s;
    -webkit-transition: all 0.5s;
}
.search-area.active {
    transition: all 0.5s;
    -webkit-transition: all 0.5s;
    opacity: 1;
    visibility: visible;
}
.search-area .container {
    height: 100%;
    display: flex;
    align-items: center;
    justify-content: center;
}
.search-form-wrapper {
    max-width: 980px;
    width: 100%;
    margin: 0px auto;
}
.search-area .searh-feild input::placeholder {
  color: #3C425F;
  opacity: 1; /* Firefox */
}

.search-area .searh-feild input:-ms-input-placeholder { /* Internet Explorer 10-11 */
 color: #3C425F;
}

.search-area .searh-feild input::-ms-input-placeholder { /* Microsoft Edge */
 color: #3C425F;
}
.search-form-wrapper form{
	border-bottom:2px solid #3C425F;
}
.search-form-wrapper form img{
	filter: brightness(0.5);
}
.search-close{
	position: absolute;
    right: 30px;
    top: 35px;
    z-index: 99;
}
.search-button button {
    border: 0px;
    background: none;
}
.searh-feild input{
	background: none;
    font-family: 'Optima Bold';
    font-size: 25px;
    color: #3C425F;
    border: 0px;
    width: 100%;
}
.searh-feild{
	width:100%;
}
.mobile-screen-button{
    background: #AFB4C9;
    position: fixed;
    width: 100%;
    bottom: 0px;
    z-index: 99999;
    left: 0px;
    transition: all .5s;
    display: none;
    -webkit-box-shadow: 0px 0px 19px -5px rgb(0 0 0 / 45%);
    -moz-box-shadow: 0px 0px 19px -5px rgba(0,0,0,0.45);
    box-shadow: 0px 0px 19px -5px rgb(0 0 0 / 45%);
}
.mobile-screen-button ul{
	margin:0px;
	padding:0px;
	font-size:0px;
	display:table;
	width:100%;
}
.mobile-screen-button ul li{
	display:table-cell;
	width:50%;
}
.mobile-screen-button ul li a{
	font-family: 'Optima Bold';
	font-size: 18px;
	letter-spacing: 2px;
	transition: all 0.5s;
	-webkit-transition: all 0.5s;
	color: #fff;
	display:block;
	text-align:center;
	padding: 8px 33px;
	border:0px;
}
.mobile-screen-button ul li a.schedule-button{
	background: #3C425F;
}

