/*************** Import Css **************/

@import "common.css";
@import "fonts.css";

/********************* Header Css ******************/
.header-area{
	background:#fff;
	padding:21px 0px;
	overflow-X:hidden;
	-webkit-box-shadow: 0px 5px 20px -9px rgba(0,0,0,0.75);
	-moz-box-shadow: 0px 5px 20px -9px rgba(0,0,0,0.75);
	box-shadow: 0px 5px 20px -9px rgba(0,0,0,0.75);
	position:absolute;
	top:0px;
	left:0px;
	width:100%;
	z-index:1;
}
.location-and-phonenumber ul{
	list-style:none;
	padding:0px;
	margin:0px;
}
.location-and-phonenumber ul li{
	display:inline-block;
	padding:0px 20px;
	border-left:2px solid #3C425F;
	line-height: 13px;
}
.location-and-phonenumber ul li a{
	font-family: 'Optima Bold';
	font-size:15px;
	color:#3C425F;
	letter-spacing:0.5px;
	transition:all 0.5s;
	-webkit-transition:all 0.5s;
}
.location-and-phonenumber ul li:last-child{
	padding-right:0px;
}
.location-and-phonenumber ul li:first-child{
	padding-left:0px;
	border:0px;
}
.location-and-phonenumber ul li a:hover{
	color:#d9a883;
	transition:all 0.5s;
	-webkit-transition:all 0.5s;
}
.menu-right-part > div{
	padding-left:25px;
}
.menu-right-part > div.menu-icon-icon{
	padding-left:35px;
	margin-top:-12px;
}
.menu-icon-icon span{
	font-family: 'Optima Bold';
	font-size:14px;
	color:#3C425F;
	letter-spacing:1px;
	position:relative;
	padding-bottom:20px;
	text-transform:uppercase;
	display:inline-block;
	line-height:15px;
}
.menu-icon-icon span:after{
	width:100%;
	height:1px;
	background:#3C425F;
	content:'';
	position:absolute;
	bottom:10px;
	left:0px;
}
.menu-icon-icon span:before{
	width:100%;
	height:1px;
	background:#3C425F;
	content:'';
	position:absolute;
	bottom:0px;
	left:0px;
}
.header-area.fixed_header {
	position: fixed;
    top:-100%;
    /*background: #0D0D0D;*/
    transition: all 0.5s;
    -webkit-transition: all 0.5s;
	width:100%;
	z-index: 99;
}
.header-area.fixed_header.stky {
    position: fixed;
    left: 0px;
    top: 0px;
    transition: all 0.5s;
    -webkit-transition: all 0.5s;
}
.header-area .container-fluid.desktop-header{
	padding:0px 30px;
}
.schedule-button-area a{
	font-family: 'Optima Bold';
	font-size:16px;
	color:#D9A883;
	letter-spacing:1px;
	text-transform:uppercase;
	transition: all 0.5s;
    -webkit-transition: all 0.5s;
}
.schedule-button-area a:hover{
	color:#3C425F;
	transition: all 0.5s;
    -webkit-transition: all 0.5s;
}
.mobile-header{
	padding:0px 25px;
	display:none;
}
.mobile-logo-area img{
	width:120px;
}
/********************* End:Header Css ******************/

/********************** Banner Area ********************/
.banner-area{
	min-height:100vh;
	position:relative;
}
.logo-area{
	margin-bottom:45px;
}
.banner-content-area{
	position:absolute;
	width:100%;
	bottom:70px;
	left:0px;
}
/********************** End:Banner Area ********************/

/********************** Natural approch area Css *********************/
.natural-approch-area{
	padding:165px 0px;
	position:relative;
}
.natural-approch-area:after{
	position:absolute;
	width:660px;
	height:141px;
	content:'';
	left:0px;
	top:50%;
	transform:translateY(-50%);
	background:url('../images/Group 6.png') no-repeat left center / contain;
	z-index: -1;
}
.natural-approch-left-part h2{
	font-family: 'Optima Bold';
	font-size:45px;
	line-height:56px;
	letter-spacing:3px;
	margin-bottom:20px;
}
.natural-approch-left-part p{
	font-family: 'Montserrat Regular ';
	font-style:italic;
	font-size:22px;
	text-transform:uppercase;
	letter-spacing:1px;
	margin-bottom:0px;
}
.natural-approch-right-part p{
	margin-bottom:0px;
	color:#777A80
}
.natural-approch-right-part p strong{
	font-family: 'Montserrat Semi bold';
	font-style:italic;
}
.natural-approch-right-part{
	padding-top:10px;
	padding-left: 30px;
}
.natural-approch-area-wrapper{
	padding:0px 40px;
}
.natural-approch-area-button{
	margin-top:110px;
}
/********************** End:Natural approch area Css *********************/

/********************** Service Css *********************/

.service-section{
	padding:200px 0px;
}
.tab-area{
	background:#fff;
	/*height:520px;*/
	z-index:9;
	position:relative;
	padding:75px 50px;
	-webkit-box-shadow: 0px 0px 17px -6px rgba(0,0,0,0.75);
	-moz-box-shadow: 0px 0px 17px -6px rgba(0,0,0,0.75);
	box-shadow: 0px 0px 17px -6px rgba(0,0,0,0.75);
	min-height:555px;
}
.each-service-image-wrapper{
	height: calc(100% + 100px);
    width: 40%;
    position: absolute;
    left: 0px;
	/*background:#f00;*/
	top:46%;
	transform:translateY(-50%);
}
.each-service{
	position:relative;
	margin-bottom:200px;
}
.each-service .row .col-lg-9{
	position:static
}
.each-service-image{
	/*display:none*/
	opacity:0;
	visibility:hidden;
	position:absolute;
	top:0px;
	left:0px;
	width:100%;
	transition:all 0.5s;
	-webkit-transition:all 0.5s;
}
.each-service-image.active{
	/*display:block*/
	opacity:1;
	visibility:visible;
	transition:all 0.5s;
	-webkit-transition:all 0.5s;
}
.tab-area h2{
	font-size:35px;
	text-transform:uppercase;
	line-height:32px;
	text-align:center;
	margin-bottom:40px;
}

.tab-area-category ul.nav.nav-tabs{
	display:block
}
.tab-area-category ul.nav.nav-tabs{
	border:0px;
}
.tab-area-category .nav-tabs .nav-item{
	margin-bottom:35px;
}
.tab-area-category .nav-tabs .nav-item:last-child{
	margin-bottom:0px;
}
.tab-area-category .nav-tabs .nav-item .nav-link{
	font-family: 'Optima Bold';
	font-size:20px;
	color:#3C425F;
	text-transform:uppercase;
	display:inline-block;
	padding:0px;
	border:0px;
	transition:all 0.5s;
	-webkit-transition:all 0.5s;
}
.tab-area-category .nav-tabs .nav-item.show .nav-link, 
.tab-area-category .nav-tabs .nav-link.active {
    border:0px;
	color:#D9A883;
}
.tab-area-category .nav-tabs .nav-link:focus, 
.tab-area-category .nav-tabs .nav-link:hover {
	color:#D9A883;
	transition:all 0.5s;
	-webkit-transition:all 0.5s;
}
.each-tab-content-area p{
	color:#777A80;
}
.each-tab-content-area p strong{
	display:block;
}
.each-tab-content-area ul{
	padding:0px;
	margin:0px;
}
.each-tab-content-area ul li{
	display:inline-block;
	width:49%;
	line-height:40px;
	white-space: nowrap;
}
.each-tab-content-area ul li a{
	font-family: 'Optima Bold';
	font-size:16px;
	color:#AFB4C9;
	text-transform: uppercase;
    letter-spacing: 1px;
	line-height:normal;
	padding-right:20px;
	background-image: url(../images/listing-arrow.svg);
    background-position: right center;
	background-repeat: no-repeat;
    background-size: 6px;
	transition:all 0.5s;
	-webkit-transition:all 0.5s;
}
.each-tab-content-area ul li a:hover {
    color: #D9A883;
	transition:all 0.5s;
	-webkit-transition:all 0.5s;
}
.tab-area-category{
	padding-right:30px;
}
.each-tab-content-area{
	padding:10px 0px 10px 30px;
	border-left:1px solid #AFB4C9
}
.tab-area-wrapper > div{
	vertical-align:middle;
}
.tab-area-bottom-button{
	margin-top:42px;
}
.tab-area-bottom-button a{
	font-family: 'Optima Bold';
	font-size:18px;
	color:#3C425F;
	text-transform:uppercase;
	letter-spacing:1px;
	transition:all 0.5s;
	-webkit-transition:all 0.5s;
}
.tab-area-bottom-button a strong{
	padding-left:15px;
}
.tab-area-bottom-button a:hover{
	color: #D9A883;
	transition:all 0.5s;
	-webkit-transition:all 0.5s;
}
.each-service:nth-child(even) .each-service-image-wrapper{
	left:auto;
	right:0px;
}
.each-tab-content-area p{
	padding-right:80px;;
}
.each-service:last-child{
	margin-bottom:0px;
}


/************************ Mobile Service area ***************/
.mobile-service-setion{
	position:relative;
}
.mobile-service-setion .each-service-image-wrapper {
    /*height: 400px;*/
    width: 100%;
    position: relative;
    left: 0px;
    top: 0%;
    transform: translateY(0%);
	padding: 50%;
}
.accordion {
	list-style: none;
	padding: 2px;
}
.accordion > li {
	display: block;
	list-style: none;
	overflow:hidden;
}
.accordion > li > span {
	display: inline-block;
	color: #3C425F;
	margin: 4px 0;
	padding: 8px 30px;
	background: url('../images/accordian-dropdown.svg') no-repeat 99.5% center;
	background-size: 12px;
	font-weight: normal;
	cursor: pointer; 
	font-size:17px;
	font-family: 'Optima Bold';
	text-transform:uppercase;
	position:relative;
	letter-spacing:2px;
}
.accordion > li > span:after{
	width:100vw;
	height:1px;
	bottom:0px;
	left:50%;
	transform:translateX(-50%);
	background:#AFB4C9;
	position:absolute;
	content:'';
}
.accordion > li > div {
	list-style: none;
	padding: 6px 0px;
	display: none; 
	overflow:auto;
	text-align:left;
}
.accordion > ul li {
	font-weight: normal;
	cursor: auto;
	padding: 0 0 0 7px;
}
.accordion a {
	text-decoration: none;
}
.accordion li > span.active {
	background: url('../images/accordian-up-arrow.svg') no-repeat 99.5% center;
	background-size: 12px;
	color:#D9A883
}
.mobile-accordian-wrapper{
	background:#fff;
	padding:50px 40px;
	text-align:center;
	margin-top: -100px;
    position: relative;
	-webkit-box-shadow: 0px 0px 16px 0px rgba(0,0,0,0.41);
	-moz-box-shadow: 0px 0px 16px 0px rgba(0,0,0,0.41);
	box-shadow: 0px 0px 16px 0px rgba(0,0,0,0.41);
	max-width:95%;
	margin-left:auto;
	margin-right:auto;
}
.mobile-accordian-wrapper h2 {
    font-size: 30px;
    line-height: 38px;
    letter-spacing: 1px;
    margin-bottom: 10px;
	text-transform:uppercase;
}
.accordion > li > div a{
	display:block;
	font-family: 'Montserrat Bold';
	font-size:15px;
	color:#AFB4C9;
	letter-spacing:1px;
	transition: all 0.5s;
	-webkit-transition: all 0.5s;
}
.accordion > li > div a:hover{
	color:#D9A883;
	transition: all 0.5s;
	-webkit-transition: all 0.5s;
}
.accordion > li > div a strong{
	padding-left:5px;
}
.each-mobile-service{
	margin-bottom:80px;
}
.mobile-service-setion{
	padding-bottom:75px;
}
.mobile-service-setion > .each-mobile-service:last-child{
	margin-bottom:0px;
}
.mobile-service-setion{
	display:none;
}
/********************** End:Service Css *********************/

